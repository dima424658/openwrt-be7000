#!/bin/sh

[ -e /lib/firmware/$FIRMWARE ] && exit 0

. /lib/functions/caldata.sh

board=$(board_name)

case "$FIRMWARE" in
"ath12k/QCN9274/hw2.0/cal-pci-0001:01:00.0.bin")
	case "$board" in
	xiaomi,be7000)
		caldata_extract "0:art" 0x26800 0x20000
		;;
	esac
	;;
"ath12k/QCN9274/hw2.0/cal-pci-0002:01:00.0.bin")
	case "$board" in
	xiaomi,be7000)
		caldata_extract "0:art" 0x4C000 0x20000
		;;
	esac
	;;
"ath12k/QCN9274/hw2.0/cal-pci-0003:01:00.0.bin")
	case "$board" in
	xiaomi,be7000)
		caldata_extract "0:art" 0x33000 0x2D000 # 0x65000 offset for 5G split
		;;
	esac
	;;
*)
	exit 1
	;;
esac